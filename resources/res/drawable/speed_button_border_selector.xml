<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="true" android:state_pressed="true">
        <shape>
            <stroke android:width="3dp" android:color="@color/secondary_2"/>
            <solid android:color="@color/secondary_2"/>
            <padding android:left="3dp" android:top="3dp" android:right="3dp" android:bottom="3dp"/>
            <corners android:topLeftRadius="35dp" android:topRightRadius="35dp" android:bottomLeftRadius="35dp" android:bottomRightRadius="35dp"/>
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape>
            <stroke android:width="3dp" android:color="@color/dark_2"/>
            <padding android:left="3dp" android:top="3dp" android:right="3dp" android:bottom="3dp"/>
            <corners android:topLeftRadius="35dp" android:topRightRadius="35dp" android:bottomLeftRadius="35dp" android:bottomRightRadius="35dp"/>
        </shape>
    </item>
    <item>
        <shape>
            <stroke android:width="3dp" android:color="@color/secondary_2"/>
            <padding android:left="3dp" android:top="3dp" android:right="3dp" android:bottom="3dp"/>
            <corners android:topLeftRadius="35dp" android:topRightRadius="35dp" android:bottomLeftRadius="35dp" android:bottomRightRadius="35dp"/>
        </shape>
    </item>
</selector>
